    "ng": "ng",
    "install": "napa",
    "start": "ng serve -p 4000",
    "test": "ng test",
    "lint": "ng lint",
    "compile_ngx_bootstrap": "babel node_modules/ngx-bootstrap -d node_modules/ngx-bootstrap --presets es2015",
    "compile_angular2_toaster": "babel node_modules/angular2-toaster -d node_modules/angular2-toaster --presets es2015",
    "compile_ng_http_cache": "babel node_modules/ng-http-cache -d node_modules/ng-http-cache --presets es2015",
    "compile_ng_angular_froala_wysiwyg": "babel node_modules/angular-froala-wysiwyg -d node_modules/angular-froala-wysiwyg --presets es2015",
    "postinstall": "npm run compile_ng_angular_froala_wysiwyg && npm run compile_ngx_bootstrap && npm run compile_angular2_toaster && npm run compile_ng_http_cache",
    "e2e": "ng e2e",
    "build:ssr": "npm run build:client-and-server-bundles && npm run webpack:server",
    "serve:ssr": "node dist/server.js",
    "ssr": "npm run build:ssr && npm run serve:ssr",
    "build:client-and-server-bundles": "ng build --prod && ng build --prod --app 1 --output-hashing=false",
    "webpack:server": "webpack --config webpack.server.config.js --progress --colors",
    "build": "npm run build:client-and-server-bundles && npm run build:server",
    "build:client": "ng build --prod --build-optimizer --app 0",
    "build:aot": "ng build --prod --app 1 ",
    "build:server": "webpack --progress --colors"
  },
